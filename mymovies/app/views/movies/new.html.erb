<h1>Movies#new</h1>
<p>Find me in app/views/movies/new.html.erb</p>
<%= form_for @movie, url: {action: "create"} do |f| %>

  <%= f.label :title %>    
  <%= f.text_field :title %>
  
  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :director %>    
  <%= f.text_field :director %>

  <%= f.label :year %>    
  <%= f.text_field :year %>

  <%= f.submit "Save" %>
<% end %>